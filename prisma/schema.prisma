// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "cargo prisma"
  output   = "../service/src/prisma.rs"
}

datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model Role {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model Menu {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
